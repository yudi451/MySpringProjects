package com.uday.learning.jpa.jpain10steps.entity;

public class User {
	private long id;
	private String name;
	private String role;
	
	
	public long getId() {
		return id;
	}
	public String getName() {
		return name;
	}
	public String getRole() {
		return role;
	}
	
	@Override
	public String toString() {
		return "User [id=" + id + ", name=" + name + ", role=" + role + "]";
	}
}
